#if div >= 2**8:
#    print("Invalid Input")

#potential pins
#pin = [8, 10, 11, 12, 13, 15, 16, 18]

x = 0 #counting up for array location
n = 7 #count down for base 2
div = 95
light = [0,0,0,0,0,0,0,0] #

while x < 8: #going through binary digits
    max = 2 ** (n) #binary base-2
    #print (max)
    if div >= max: #if divisible
        div = div % max #then
        light[x] = 1
    else:
        light[x] = 0
    x = x + 1
    n = n - 1
print (light)
